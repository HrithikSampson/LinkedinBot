{
        "manifest_version": 3,
        "name": "Linkedin Extension",
        "version": "1.0.0",
        "description": "Linkedin Extension to automate connections",
        "action": {
                "default_popup": "popup.html"
        },
        "content_scripts": [
                {
                        "js": ["/scripts/content_script.js"],
                        "matches":["https://www.linkedin.com/search/results/people/*","<all_urls>"]
                }
        ],
        "background": [
                {
                        "js": ["background.js"]
                }
        ],
        "permissions": [
                "activeTab",
                "tabs",
                "nativeMessaging",
                "scripting"
        ]
}